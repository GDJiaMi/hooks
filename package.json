{
  "name": "@gdjiami/hooks",
  "version": "0.4.3",
  "description": "react hooks for mygzb.com",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build:clean": "rm -rf es lib",
    "build:es": "tsc -b tsconfig-build.json",
    "build:commonjs": "tsc -b tsconfig-build.cm.json",
    "build": "npm run build:clean && npm run build:es && npm run build:commonjs",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "doc:dev": "docz dev",
    "doc:build": "docz build"
  },
  "files": [
    "es",
    "lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/!(helper)*.ts?(x)"
    ],
    "roots": [
      "src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://code.ejiahe.com:82/WEB/jm-hooks.git"
  },
  "keywords": [
    "react",
    "react-hooks",
    "mygzb",
    "hooks"
  ],
  "author": "ivan",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0",
    "tslib": ">=1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "babel-jest": "^24.4.0",
    "docz": "^1.0.0-rc.3",
    "docz-theme-default": "^1.0.0-rc.3",
    "husky": "^1.2.1",
    "jest": "^24.4.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hooks-testing-library": "^0.3.6",
    "react-spring": "^8.0.18",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {}
}
