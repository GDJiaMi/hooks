{
  "name": "@gdjiami/hooks",
  "version": "0.2.2",
  "description": "react hooks for mygzb.com",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -b tsconfig-build.json && cp package.json dist && cp README.md dist",
    "pub": "npm run build && cd dist && npm publish",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/!(helper)*.ts?(x)"
    ],
    "roots": [
      "src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://code.ejiahe.com:82/WEB/jm-hooks.git"
  },
  "keywords": [
    "react",
    "react-hooks",
    "mygzb",
    "hooks"
  ],
  "author": "ivan",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0",
    "tslib": ">=1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "babel-jest": "^24.4.0",
    "docz": "^0.13.5",
    "docz-theme-default": "^0.13.5",
    "husky": "^1.2.1",
    "jest": "^24.4.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hooks-testing-library": "^0.3.6",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "react-hooks-global-state": "^0.7.0"
  }
}
